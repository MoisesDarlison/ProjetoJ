{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "semi": ["warn", "never"],
    "quotes": ["warn", "single"],
    "no-unused-vars": "warn",
    "no-multiple-empty-lines": [
      "warn",
      {
        "max": 1
      }
    ]
  }
}
